{
  "name": "AI News Processing Pipeline",
  "description": "Complete workflow: Google News â†’ Article Scraping â†’ AI Summarization",
  "version": "1.0.0",
  "steps": [
    {
      "node": "google-news-scraper",
      "inputs": {
        "api_token": "your-apify-token-here",
        "keywords": ["AI", "artificial intelligence", "machine learning"],
        "max_news": 5,
        "time_period": "Last 24 hours",
        "region_code": "United States (English)",
        "extract_descriptions": true
      }
    },
    {
      "assembly": {
        "selected_article_url": {
          "action": "select_random",
          "from": "articles",
          "extract": "url"
        }
      },
      "source": "google-news-scraper",
      "name": "article_selector",
      "description": "ðŸ”§ ASSEMBLY: Randomly selects one article URL from the news results"
    },
    {
      "node": "article-page-scraper",
      "inputs": {
        "url": "{{article_selector.selected_article_url}}",
        "output_filename": "ai_article.html",
        "headless": true
      }
    },
    {
      "assembly": {
        "clean_article_text": "article_text"
      },
      "source": "article-page-scraper",
      "name": "text_extractor",
      "description": "ðŸ”§ ASSEMBLY: Extracts clean article text for summarization"
    },
    {
      "node": "article-processor",
      "inputs": {
        "article_text": "{{text_extractor.clean_article_text}}",
        "openrouter_api_key": "your-openrouter-key-here",
        "model": "qwen/qwen3-coder:free"
      }
    }
  ]
}